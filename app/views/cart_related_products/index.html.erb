<h1>Listing cart_related_products</h1>

<table>
  <tr>
    <th>Product</th>
    <th>Related product</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cart_related_products.each do |cart_related_product| %>
  <tr>
    <td><%= cart_related_product.product_id %></td>
    <td><%= cart_related_product.related_product_id %></td>
    <td><%= link_to 'Show', cart_related_product %></td>
    <td><%= link_to 'Edit', edit_cart_related_product_path(cart_related_product) %></td>
    <td><%= link_to 'Destroy', cart_related_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

