<h1>Listing ambassadors</h1>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Discipline</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ambassadors.each do |ambassador| %>
  <tr>
    <td><%= ambassador.first_name %></td>
    <td><%= ambassador.last_name %></td>
    <td><%= ambassador.discipline %></td>
    <td><%= link_to 'Show', ambassador %></td>
    <td><%= link_to 'Edit', edit_ambassador_path(ambassador) %></td>
    <td><%= link_to 'Destroy', ambassador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Ambassador', new_ambassador_path %>
